package simplefsmtest

import static extension fr.inria.diverse.melange.lib.MetamodelExtensions.*

language Fsm {
	ecore "tests-inputs/metamodels/FSM.ecore" withGenmodel "tests-inputs/metamodels/FSM.genmodel"
	exactType FsmMT
}

language TimedFsm {
	ecore "tests-inputs/metamodels/TimedFSM.ecore" withGenmodel "tests-inputs/metamodels/TimedFSM.genmodel"
	exactType TimedFsmMT
}

transformation simplefsmtest.fsmmt.FSM getRoot(FsmMT fsm) {
	return fsm.contents.head as simplefsmtest.fsmmt.FSM
}

transformation FsmMT loadFsm()  { return      Fsm.load("tests-inputs/models/Simple.fsm", FsmMT) }
transformation FsmMT loadTFsm() { return TimedFsm.load("tests-inputs/models/Simple.fsm", FsmMT) }

@Main
transformation test() {}

